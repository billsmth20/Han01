void actionOne(){
  unsigned char PatternBuf[ROW_SIZE];
  int PatternIdx;
  byte DisplayTime;
  unsigned long EndTime;
  int patbufidx;
  int ledrow;
  int ledcol;
  
  PatternIdx = 0;

  int r=0;
  int g=0;
  int b=0;

  int r_co=0;
  int g_co=0;
  int b_co=0;

  do {
    memcpy_P( PatternBuf, PatternTable+PatternIdx, ROW_SIZE );
    PatternIdx += ROW_SIZE;
    
    r_co = pgm_read_byte_near( PatternTable + PatternIdx++ );
    g_co = pgm_read_byte_near( PatternTable + PatternIdx++ );
    b_co = pgm_read_byte_near( PatternTable + PatternIdx++ );
    
    DisplayTime = pgm_read_byte_near( PatternTable + PatternIdx++ );
    
    EndTime = millis() + ((unsigned long) DisplayTime) * TIMECONST;
    r=r_co;
    g=g_co;
    b=b_co;
//    r=random(255);
//    g=random(255);
//    b=random(255);
    while ( millis() < EndTime ) {
      patbufidx = 0;

      for (ledrow=0; ledrow<ROW_SIZE; ledrow++) {
        strips[ledrow].begin();
        for (ledcol=0; ledcol<COLUMN_SIZE; ledcol++) {
         if((PatternBuf[ledrow] & (1 << ledcol))>0){
            strips[ledrow].setPixelColor(ledcol, strips[ledrow].Color(r, g, b));
         }else{
            strips[ledrow].setPixelColor(ledcol, strips[ledrow].Color(0, 0, 0));
         }
        }
        patbufidx++;
      }
      for(ledrow=0; ledrow<ROW_SIZE; ledrow++) {
        strips[ledrow].show();
      }
      delayMicroseconds( PLANETIME );
    }
  } while (DisplayTime > 0);
}
